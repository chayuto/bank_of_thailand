            "name": "keyword",
            "in": "query",
            "description": "keyword for search series",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "200 OK",
            "content": {
              "application/xml": {
                "schema": {
